{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "/",
  "type": "object",
  "javaInterfaces" : ["com.t1t.kong.model.KongConfigValue"],
  "properties": {
    "aws_key": {
      "type": "string"
    },
    "aws_secret": {
      "type": "string"
    },
    "function_name": {
      "type": "string"
    },
    "aws_region": {
      "enum": [
        "us-east-1",
        "us-east-2",
        "ap-northeast-1",
        "ap-northeast-2",
        "us-west-2",
        "ap-southeast-1",
        "ap-southeast-2",
        "eu-central-1",
        "eu-west-1"
      ]
    },
    "qualifier": {
      "type": "string"
    },
    "invocation_type": {
      "enum": [
        "RequestResponse",
        "Event",
        "DryRun"
      ]
    },
    "log_type": {
      "enum": [
        "Tail",
        "None"
      ]
    },
    "timeout": {
      "type": "integer"
    },
    "keepalive": {
      "type": "integer"
    }
  }
}