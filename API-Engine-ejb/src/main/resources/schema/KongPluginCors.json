{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "/",
  "type": "object",
  "extends" : {
    "$ref" : "KongConfigValue.json"
  },
  "properties": {
    "methods": {
      "type": "array",
      "enum": [
        "HEAD",
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE"
      ]
    },
    "credentials": {
      "type": "boolean",
      "default": false
    },
    "exposed_headers": {
      "type": "array"
    },
    "origin": {
      "type": "string"
    },
    "max_age": {
      "type": "number"
    },
    "preflight_continue": {
      "type": "boolean",
      "default": false
    },
    "headers": {
      "type": "array"
    }
  }
}