        "running": {
          "type": "integer"
        },
        "pending": {
          "type": "integer"
        }
      },
      "required": [
        "running",
        "pending"
      ]
    },
    "version": {
      "type": "string"
    },
    "configuration": {
      "type": "object",
      "properties": {
        "send_anonymous_reports": {
          "type": "boolean"
        },
        "dns_resolver": {
          "type": "object",
          "properties": {
            "address": {
              "type": "string"
            },
            "dnsmasq": {
              "type": "boolean"
            },
            "port": {
              "type": "integer"
            }
          },
          "required": [
            "address",
            "dnsmasq",
            "port"
          ]
        },
        "postgres": {
          "type": "object",
          "properties": {
            "host": {
              "type": "string"
            },
            "database": {
              "type": "string"
            },
            "user": {
              "type": "string"
            },
            "port": {
              "type": "integer"
            }
          },
          "required": [
            "host",
            "database",
            "user",
            "port"
          ]
        },
        "cluster_listen": {
          "type": "string"
        },
        "cluster": {
          "type": "object",
          "properties": {
            "auto-join": {
              "type": "boolean"
            },
            "profile": {
              "type": "string"
            },
            "ttl_on_failure": {
              "type": "integer"
            }
          },
          "required": [
            "auto-join",
            "profile",
            "ttl_on_failure"
          ]
        },
        "proxy_listen": {
          "type": "string"
        },
        "nginx": {
          "type": "string"
        },
        "custom_plugins": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "proxy_listen_ssl": {
          "type": "string"
        },
        "dns_resolvers_available": {
          "type": "object",
          "properties": {
            "server": {
              "type": "object",
              "properties": {
                "address": {
                  "type": "string"
                }
              },
              "required": [
                "address"
              ]
            },
            "dnsmasq": {
              "type": "object",
              "properties": {
                "port": {
                  "type": "integer"
                }
              },
              "required": [
                "port"
              ]
            }
          },
          "required": [
            "server",
            "dnsmasq"
          ]
        },
        "plugins": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "pid_file": {
          "type": "string"
        },
        "dao_config": {
          "type": "object",
          "properties": {
            "timeout": {
              "type": "integer"
            },
            "data_centers": {
              "type": "object",
              "properties": {}
            },
            "port": {
              "type": "integer"
            },
            "ssl": {
              "type": "object",
              "properties": {
                "verify": {
                  "type": "boolean"
                },
                "enabled": {
                  "type": "boolean"
                }
              },
              "required": [
                "verify",
                "enabled"
              ]
            },
            "consistency": {
              "type": "string"
            },
            "contact_points": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "replication_strategy": {
              "type": "string"
            },
            "replication_factor": {
              "type": "integer"
            },
            "keyspace": {
              "type": "string"
            }
          },
          "required": [
            "timeout",
            "data_centers",
            "port",
            "ssl",
            "consistency",
            "contact_points",
            "replication_strategy",
            "replication_factor",
            "keyspace"
          ]
        },
        "nginx_working_dir": {
          "type": "string"
        },
        "admin_api_listen": {
          "type": "string"
        },
        "cassandra": {
          "type": "object",
          "properties": {
            "timeout": {
              "type": "integer"
            },
            "data_centers": {
              "type": "object",
              "properties": {}
            },
            "port": {
              "type": "integer"
            },
            "ssl": {
              "type": "object",
              "properties": {
                "verify": {
                  "type": "boolean"
                },
                "enabled": {
                  "type": "boolean"
                }
              },
              "required": [
                "verify",
                "enabled"
              ]
            },
            "consistency": {
              "type": "string"
            },
            "contact_points": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "replication_strategy": {
              "type": "string"
            },
            "replication_factor": {
              "type": "integer"
            },
            "keyspace": {
              "type": "string"
            }
          },
          "required": [
            "timeout",
            "data_centers",
            "port",
            "ssl",
            "consistency",
            "contact_points",
            "replication_strategy",
            "replication_factor",
            "keyspace"
          ]
        },
        "ssl_key_path": {
          "type": "string"
        },
        "ssl_cert_path": {
          "type": "string"
        },
        "database": {
          "type": "string"
        },
        "memory_cache_size": {
          "type": "integer"
        },
        "cluster_listen_rpc": {
          "type": "string"
        }
      },
      "required": [
        "send_anonymous_reports",
        "dns_resolver",
        "postgres",
        "cluster_listen",
        "cluster",
        "proxy_listen",
        "nginx",
        "custom_plugins",
        "proxy_listen_ssl",
        "dns_resolvers_available",
        "plugins",
        "pid_file",
        "dao_config",
        "nginx_working_dir",
        "admin_api_listen",
        "cassandra",
        "ssl_key_path",
        "ssl_cert_path",
        "database",
        "memory_cache_size",
        "cluster_listen_rpc"
      ]
    },
    "lua_version": {
      "type": "string"
    },
    "tagline": {
      "type": "string"
    },
    "hostname": {
      "type": "string"
    },
    "plugins": {
      "type": "object",
      "properties": {
        "enabled_in_cluster": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "available_on_server": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "enabled_in_cluster",
        "available_on_server"
      ]
    }
